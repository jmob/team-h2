    <class>de.rentajet.entity.Kundenberater</class>
    <class>de.rentajet.entity.Benutzergruppe</class>
    <class>de.rentajet.entity.Kunde</class>
    <class>de.rentajet.entity.Flugzeugtyp</class>
    <class>de.rentajet.entity.Benutzer</class>
    <class>de.rentajet.entity.Flugzeug</class>
    <class>de.rentajet.entity.Mitarbkunde</class>
    <class>de.rentajet.entity.Ort</class>
    <class>de.rentajet.entity.Staat</class>
    <class>de.rentajet.entity.Flugzeugkosten</class>
    <class>de.rentajet.entity.Firma</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/teamh2?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
